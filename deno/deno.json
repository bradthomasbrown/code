{
    "compilerOptions": {
        "strict": true,
        "strictBindCallApply": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "noUncheckedIndexedAccess": true,
        "exactOptionalPropertyTypes": true,
        "noPropertyAccessFromIndexSignature": true,
        "noImplicitOverride": true,
        "noImplicitAny": true
    },
    "lint": {
        "adjacent-overload-signatures": true,
        "ban-ts-comment": true,
        "ban-types": true,
        "ban-unknown-rule-code": true,
        "ban-untagged-ignore": true,
        "ban-unused-ignore": true,
        "constructor-super": true,
        "for-direction": true,
        "getter-return": true,
        "no-array-constructor": true,
        "no-async-promise-executor": true,
        "no-await-in-sync-fn": true,
        "no-case-declarations": true,
        "no-class-assign": true,
        "no-compare-neg-zero": true,
        "no-cond-assign": true,
        "no-constant-condition": true,
        "no-control-regex": true,
        "no-debugger": true,
        "no-delete-var": true,
        "no-deprecated-deno-api": true,
        "no-dupe-args": true,
        "no-dupe-class-members": true,
        "no-dupe-else-if": true,
        "no-dupe-keys": true,
        "no-duplicate-case": true,
        "no-empty": true,
        "no-empty-character-class": true,
        "no-empty-enum": true,
        "no-empty-interface": true,
        "no-empty-pattern": true,
        "no-ex-assign": true,
        "no-explicit-any": true,
        "no-extra-boolean-cast": true,
        "no-extra-non-null-assertion": true,
        "no-fallthrough": true,
        "no-func-assign": true,
        "no-global-assign": true,
        "no-import-assertions": true,
        "no-import-assign": true,
        "no-inner-declarations": true,
        "no-invalid-regexp": true,
        "no-invalid-triple-slash-reference": true,
        "no-irregular-whitespace": true,
        "no-misused-new": true,
        "no-namespace": true,
        "no-new-symbol": true,
        "no-node-globals": true,
        "no-obj-calls": true,
        "no-octal": true,
        "no-prototype-builtins": true,
        "no-redeclare": true,
        "no-regex-spaces": true,
        "no-self-assign": true,
        "no-setter-return": true,
        "no-shadow-restricted-names": true,
        "no-this-alias": true,
        "no-this-before-super": true,
        "no-unreachable": true,
        "no-unsafe-finally": true,
        "no-unsafe-negation": true,
        "no-unused-labels": true,
        "no-unused-vars": true,
        "no-var": true,
        "no-window": true,
        "no-window-prefix": true,
        "no-with": true,
        "prefer-as-const": true,
        "prefer-const": true,
        "prefer-namespace-keyword": true,
        "require-await": true,
        "require-yield": true,
        "use-isnan": true,
        "valid-typeof": true
    }
}